{
  "version": 2,
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/server/index.ts" },
    { "src": "/(.*)", "dest": "/server/index.ts" }
  ],
  "functions": {
    "server/index.ts": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "outputDirectory": "dist/public"
}