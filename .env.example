# API Keys Configuration
# Copy this file to .env and fill in your actual credentials for local development
# .env should NEVER be committed to version control

# Backend-only variables (no VITE_ prefix - not exposed to frontend)
XAI_API_KEY=your_xai_api_key_here
GMAIL_USER=your_gmail_address@gmail.com
GMAIL_APP_PASSWORD=your_gmail_app_password_here

# Frontend-accessible variables (must start with VITE_ prefix)
# These can be accessed in React code using: import.meta.env.VITE_VARIABLE_NAME
VITE_API_URL=http://localhost:5000
